{
    "name": "flood-risk-backend",
    "version": "1.0.0",
    "description": "Backend API for Flood Risk Management System",
    "main": "src/server.js",
    "scripts": {
        "build": "tsc",
        "start": "node dist/server.js",
        "dev": "nodemon --exec ts-node src/server.ts",
        "dev:js": "nodemon src/server.js",
        "test": "jest",
        "test:api": "node test-api.js",
        "test:db": "node test-db.js",
        "seed": "ts-node scripts/seed.ts",
        "seed:js": "node scripts/seed.js",
        "type-check": "tsc --noEmit",
        "check-env": "node check-env.js",
        "setup": "npm run check-env && npm run seed",
        "setup:full": "./setup.sh",
        "docker:build": "docker build -t flood-risk-backend .",
        "docker:run": "docker run -p 5000:5000 flood-risk-backend",
        "docker:dev": "docker-compose -f docker-compose.dev.yml up --build",
        "docker:prod": "docker-compose -f docker-compose.prod.yml up --build -d",
        "docker:azure": "docker-compose -f docker-compose.azure.yml up --build -d",
        "docker:stop": "docker-compose down",
        "docker:clean": "docker system prune -f && docker volume prune -f",
        "docker:validate": "./validate-docker.sh",
        "test:external-apis": "ts-node scripts/test-external-apis.ts"
    },
    "keywords": [
        "flood",
        "risk",
        "management",
        "api",
        "express"
    ],
    "author": "Flood Risk Team",
    "license": "MIT",
    "dependencies": {
        "axios": "^1.13.2",
        "bcrypt": "^5.1.1",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "express-rate-limit": "^7.4.0",
        "express-validator": "^7.3.1",
        "helmet": "^7.1.0",
        "jsonwebtoken": "^9.0.2",
        "mongoose": "^8.5.2",
        "multer": "^1.4.5-lts.1",
        "pino": "^9.3.2",
        "pino-pretty": "^11.2.2",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.2",
        "@types/compression": "^1.7.5",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/express-validator": "^2.20.33",
        "@types/jest": "^29.5.12",
        "@types/jsonwebtoken": "^9.0.6",
        "@types/multer": "^1.4.12",
        "@types/node": "^22.5.4",
        "@types/supertest": "^6.0.2",
        "jest": "^29.7.0",
        "nodemon": "^3.1.4",
        "supertest": "^7.0.0",
        "ts-node": "^10.9.2",
        "typescript": "^5.5.4"
    },
    "engines": {
        "node": ">=18.0.0"
    }
}
